{
  "Audio": {
    "EnableSound": true
  },
  "Core": {
    "AssetsRootDirectoryPath": "Assets",
    "CustomGameLoopSteps": [
      "ShinobiDojo.CharacterControllerSystem",
      "ShinobiDojo.CharacterPhysicsSystem"
    ],
    "FixedUpdatesPerFrameLimit": 30,
    "FixedUpdatesPerSecond": 60,
    "LogLevel": "Debug",
    "ShowAllEntitiesCount": true,
    "ShowRootEntitiesCount": true,
    "ShowFps": true,
    "ShowFrameTime": true,
    "ShowGameLoopStatistics": true,
    "ShowTotalFrames": true,
    "ShowTotalTime": true,
    "StartUpSceneBehavior": "Main"
  },
  "Physics": {
    "RenderCollisionGeometry": true
  },
  "Rendering": {
    "EnableVSync": false,
    "ScreenHeight": 720,
    "ScreenWidth": 1280,
    "SortingLayersOrder": [
      "Default"
    ]
  }
}